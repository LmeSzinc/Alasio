{
  "card-Emulator": {
    "_info": {"group": "Emulator", "arg": "_info"},
    "Emulator_Serial": {
      "task": "Device",
      "group": "Emulator",
      "arg": "Serial",
      "dt": "input",
      "value": "auto"
    },
    "Emulator_PackageName": {
      "task": "Device",
      "group": "Emulator",
      "arg": "PackageName",
      "dt": "select",
      "value": "auto",
      "option": ["auto"]
    },
    "Emulator_ServerName": {
      "task": "Device",
      "group": "Emulator",
      "arg": "ServerName",
      "dt": "select",
      "value": "disabled",
      "option": ["disabled"]
    },
    "Emulator_ScreenshotMethod": {
      "task": "Device",
      "group": "Emulator",
      "arg": "ScreenshotMethod",
      "dt": "select",
      "value": "auto",
      "option": ["auto", "ADB", "ADB_nc", "uiautomator2", "aScreenCap", "aScreenCap_nc", "DroidCast", "DroidCast_raw", "nemu_ipc", "ldopengl"]
    },
    "Emulator_ControlMethod": {
      "task": "Device",
      "group": "Emulator",
      "arg": "ControlMethod",
      "dt": "select",
      "value": "MaaTouch",
      "option": ["ADB", "uiautomator2", "minitouch", "Hermit", "MaaTouch"]
    },
    "Emulator_ScreenshotDedithering": {
      "task": "Device",
      "group": "Emulator",
      "arg": "ScreenshotDedithering",
      "dt": "checkbox",
      "value": false
