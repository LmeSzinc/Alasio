  "card-Optimization": {
    "_info": {"group": "Optimization", "arg": "_info"},
    "Optimization_ScreenshotInterval": {
      "task": "Device",
      "group": "Optimization",
      "arg": "ScreenshotInterval",
      "dt": "input-float",
